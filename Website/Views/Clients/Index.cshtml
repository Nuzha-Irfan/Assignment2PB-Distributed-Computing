@{
    @using Website.Models;
    @model IEnumerable<Job>;

}

<h1>Connected Client</h1>
<div id="tableview">
    <table width="100px" height="100px" id="table">
        <tr>

            <th>Name</th>
            <th>IP</th>
            <th>Port</th>
        </tr>

        @foreach (var Job in Model)
        {
            <tr>

                <td>@Job.clientName</td>
                <td>@Job.port</td>
                <td>@Job.ipAddress</td>

            </tr>
        }
    </table>
</div>


